pipeline { 
    agent any 
    stages { 
        stage('Build'){ 
            steps { 
                sh "ls"
                echo 'Hello Jenkins Build stage'
            } 
        } 
        stage('Test'){ 
            steps { 
               sh “pwd” 
              echo 'Test stage'
            } 
        } 
         stage('Deploy'){ 
            steps { 
                sh "ls"
                echo 'Deploy Stage'
            } 
         }
    }
}
